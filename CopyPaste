CREATE SCHEMA IF NOT EXISTS `Projecao`;
USE `Projecao`;

CREATE TABLE IF NOT EXISTS `Aluno`(
`id` INT NOT NULL,
`nome` VARCHAR(255),
`email` VARCHAR(255),
`data_nascimento` DATETIME,
PRIMARY KEY (id)
);
CREATE TABLE IF NOT EXISTS `Turma`(
`id` INT NOT NULL,
`inicio` DATETIME,
`fim` DATETIME,
`observacoes` LONGTEXT,
PRIMARY KEY (id)
);

CREATE TABLE IF NOT EXISTS `AlunoTurma`
